{
    "manifest_version": 3,
    "name": "二维码生成器",
    "version": "2.0",
    "description": "一个用来把当前浏览页面地址生成二维码的小插件",
    "icons": {
        "48": "qrcode_48x48.png"
    },
    "action": {
        "default_icon": "qrcode_19x19.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs"
    ]
}